 "html": "{%- from \"templates/print_formats/standard_macros.html\" import add_header, render_field -%} {%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead, footer) -%} {% if letter_head and not no_letterhead %} {{ letter_head }} {% endif %} {%- endmacro -%} {% for page in layout %}\n<div id=\"header-html\" class=\"hidden-pdf\">\n    {{ add_header(loop.index, layout|len,doc,letter_head, no_letterhead, footer) }}\n</div>\n{%- endfor -%}\n\n<small>\n\t<div class=\"row\">        \n\t\t <div class=\"col-xs-6\">  \n\t\t\t<div class=\"col-xs-12 text-left\">  \n\t\t\t\t<h3>{{ doc.supplier_name or '' }} </h3>\n\t\t\t\t{{ doc.address_display or \"\" }}\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"col-xs-6\">            \n\t\t\t<div class=\"row\">\n\t\t\t  <h3 class=\"col-xs-12 text-right\">{{ doc.select_print_heading or \"Purchase Invoice\" }}</h3>\n\t\t\t</div>\n\t\t\t<div class=\"row\">\n\t\t\t\t<div class=\"col-xs-12 text-right\"><big>{{ doc.name or '' }}</big> </div>\n\t\t\t</div>\n\t\t\t<div class=\"row\" >     <div class=\"col-xs-12 text-right\">       \n\t\t\t\t<big>Date: </big> {{ doc.get_formatted(\"posting_date\") or '' }} </div>              \n\t\t\t</div>        \n\t\t</div>\n\t</div>\n\n\t<hr>\n\t<table class=\"table table-condensed table-hover table-bordered\">\n\t\t<tbody>\n\t\t\t<tr>\n\t\t\t\t<th class=\"text-center\">Item Code</th>\n\t\t\t\t<th class=\"text-center\">Name</th>\n\t\t\t\t<th class=\"text-center\" colspan=\"2\">Received</th>\n\t\t\t\t<th class=\"text-center\" colspan=\"2\">Per Unit</th>\n\t\t\t\t<th class=\"text-center\">Unit Rate</th>\n\t\t\t\t<th class=\"text-center\">Amount</th>\n\t\t\t</tr>\n\t\t\t{%- for row in doc.items -%}\n\t\t\t\t{% if row.qty >= 0 %}\n\t\t\t\t\t<tr>\n\t\t\t\t\t\t<td style=\"\">{{ row.item_code }}</td>\n\t\t\t\t\t\t<td style=\"width: 28%;\"><b>{{ row.item_name or '' }} </b> </td>\n\t\t\t\t\t\t<td style=\"width: 9%; text-align: center;\" class=\"text-nowrap\">{{ row.get_formatted('qty') }}</td>\n\t\t\t\t\t\t<td style=\"width: 9%; text-align: center;\" class=\"text-nowrap\">{{ row.uom }}</td>\n\t\t\t\t\t\t<td style=\"width: 9%; text-align: center;\" class=\"text-nowrap\">{{ row.get_formatted('alt_uom_size') }}</td>\n\t\t\t\t\t\t<td style=\"width: 9%; text-align: center;\" class=\"text-nowrap\">{{ row.alt_uom }}</td>\n\t\t\t\t\t\t<td style=\"width: 10%; text-align: right;\" class=\"text-nowrap\">{{ frappe.format_value(row.amount / row.qty if row.qty else row.rate, row.meta.get_field(\"rate\")) }}</td>\n\t\t\t\t\t\t<td style=\"width: 22%; text-align: right;\" class=\"text-nowrap\">{{ row.get_formatted(\"amount\", doc) }}</td>\n\n\t\t\t\t\t</tr>\n\t\t\t\t{% endif %}\t\n\t\t\t{%- endfor -%}\n\t\t\t<tr>\n\t\t\t\t<th colspan=\"2\" class=\"text-right\">Total:</th>\n\t\t\t\t<th class=\"text-center\">{{ doc.get_formatted('total_qty') }}</th>\n\t\t\t\t<th class=\"text-center\"></th>\n\t\t\t\t<th class=\"text-right\"></th>\n\t\t\t\t<th class=\"text-right\"></th>\n\t\t\t\t<th class=\"text-right\"></th>\n\t\t\t\t<th class=\"text-right\">{{ doc.get_formatted(\"total\") or '' }}</th>\n\n\t\t\t</tr>\n\t\t</tbody>\n\t</table>\n</small>", 
